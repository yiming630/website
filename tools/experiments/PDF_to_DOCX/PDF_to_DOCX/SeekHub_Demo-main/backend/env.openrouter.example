# OpenRouter Configuration (推荐用于中国大陆地区)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=google/gemini-pro
OPENROUTER_FALLBACK_MODEL=google/gemini-flash-1.5
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Local Storage Configuration (推荐用于百度服务器)
USE_LOCAL_STORAGE=true
LOCAL_STORAGE_ROOT=/data/seekhub/storage
PUBLIC_URL_BASE=http://your-baidu-server.com/files

# Google Cloud Configuration (保留兼容性，可选)
# GOOGLE_APPLICATION_CREDENTIALS=seekhub-demo-9d255b940d24.json
# FIRESTORE_PROJECT_ID=seekhub-demo
# FIRESTORE_DATABASE_ID=(default)

# Cloud Pub/Sub Configuration (可选)
# PUBSUB_PROJECT_ID=seekhub-demo
# CHAPTER_TOPIC=chapter-translation-topic
# CHAPTER_SUBSCRIPTION=chapter-translation-subscription
# COMBINATION_TOPIC=combination-topic
# COMBINATION_SUBSCRIPTION=combination-subscription

# Cloud Storage Configuration (如果USE_LOCAL_STORAGE=false)
# GCS_BUCKET_NAME=seekhub-demo-test1

# Translation Configuration
MAX_RETRIES=3
RATE_LIMIT_DELAY=1.0
BATCH_SIZE=5

# Worker Configuration
MAX_WORKERS=20
WORKER_TIMEOUT=300

# High-Speed Optimization Settings
MAX_CONCURRENT_REQUESTS=30
CONNECTION_POOL_SIZE=100
